unit uProjeto_Controller;

interface

uses
  System.Generics.Collections, System.SysUtils;

type

  TProjeto = class

    private
      FId    : Integer;
      FNome  : String;
      FValor : Currency;

    public
      property Id    : Integer  read FId    write FId;
      property Nome  : String   read FNome  write FNome;
      property Valor : Currency read FValor write FValor;
end;

implementation

end.
