unit uIGeraSql_Interface;

interface

uses System.Classes;

type
  //Implementação da interface ISubstitui com função única
  IGeraSql = interface
    procedure SetColunas(Colunas: TStringList);
    procedure SetTabela(Tabela: TStringList);
    procedure SetCondicoes(Condicoes: TStringList);
    function  GeraSQL : TStringList;
  end;

implementation

end.
